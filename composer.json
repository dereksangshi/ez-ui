{
    "name": "ez/ui",
    "type": "library",
    "description": "Html Ui",
    "keywords": ["ui", "data", "container"],
    "homepage": "https://bitbucket.org/dereksangshi2000/ez-ui",
    "license": "MIT",
    "authors": [
        {"name": "Derek Li", "email": "dereksangshi2000@gmail.com.au"}
    ],
    "require": {
        "php": ">=5.3.2",
        "ez/html": "1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "symfony/console": "~2.0"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "http://satis.internal.balancenet.com.au"
        }
    ],
    "suggest": {
        "symfony/console": "For helpful console commands to test."
    },
    "autoload": {
        "psr-0": { "Ez\\Ui\\": "src/" }
    },
    "archive": {
        "exclude": ["!vendor", "tests", "*phpunit.xml", ".travis.yml", "build.xml", "build.properties", "composer.phar"]
    }
}
